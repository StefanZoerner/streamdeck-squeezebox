{
  "Name": "Squeezebox",
  "Description": "Simple actions to control a local Squeezebox server",
  "Version": "0.1",
  "Author": "Stefan ZÃ¶rner",
  "SDKVersion": 2,

  "Category": "Squeezebox",
  "CategoryIcon": "images/Category",
  "Icon": "images/Category",

  "CodePath": "streamdeck-squeezebox",
  "URL": "",

  "Actions": [
    {
      "Name": "Check Configuration",
      "Icon": "images/ConfigureAction",
      "SupportedInMultiActions": false,
      "UUID": "de.szoerner.streamdeck.squeezebox.actions.configure",
      "States": [
        {
          "Image": "images/ConfigureKey"
        }
      ]
    },
    {
      "Name": "Play",
      "Icon": "images/PlayAction",
      "SupportedInMultiActions": false,
      "UUID": "de.szoerner.streamdeck.squeezebox.actions.play",
      "PropertyInspectorPath": "html/player_selection.html",
      "States": [{
        "Image": "images/PlayKey"
      }]
    },
    {
      "Name": "Pause",
      "Icon": "images/PauseAction",
      "SupportedInMultiActions": false,
      "UUID": "de.szoerner.streamdeck.squeezebox.actions.pause",
      "PropertyInspectorPath": "html/player_selection.html",
      "States": [{
        "Image": "images/PauseKey"
      }]
    },
    {
      "Name": "Play Toggle",
      "Icon": "images/PlayToggleAction",
      "SupportedInMultiActions": false,
      "UUID": "de.szoerner.streamdeck.squeezebox.actions.playtoggle",
      "PropertyInspectorPath": "html/player_selection.html",
      "States": [
        {
          "Image": "images/PlayKey"
        }
      ]
    },
    {
      "Name": "Volume Up",
      "Icon": "images/actions/volume_up",
      "SupportedInMultiActions": false,
      "UUID": "de.szoerner.streamdeck.squeezebox.actions.volumeup",
